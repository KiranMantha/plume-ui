{
  "name": "@plumejs/ui",
  "version": "3.0.1",
  "description": "A collection of UI components built using PlumeJs.",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run lint && rimraf dist && npm run build:mjs && npm run build:cjs",
    "build:mjs": "tsc -p tsconfig.json && copyfiles src/**/*.scss dist/mjs",
    "build:cjs": "tsc -p tsconfig-cjs.json && copyfiles src/**/*.scss dist/cjs",
    "lint": "eslint . --ext .ts --fix",
    "patch-update": "npm version patch",
    "tag": "git push --follow-tags",
    "tag-release": "npm run patch-update && npm run tag"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "KiranMantha <kiranv.mantha@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/KiranMantha/plumejs-ui"
  },
  "bugs": {
    "url": "https://github.com/KiranMantha/plumejs-ui/issues"
  },
  "homepage": "https://github.com/KiranMantha/plumejs-ui#readme",
  "keywords": [
    "plumejs",
    "plumejs-ui"
  ],
  "dependencies": {
    "@plumejs/core": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ttypescript": "^1.5.12",
    "typescript": "^4.5.2"
  }
}
